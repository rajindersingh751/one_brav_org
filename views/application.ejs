<body ng-controller="indexCtrl" layout="column">

  <div class='preloader'>
    <div class='loader'>
      <div class='loader--text'></div>
    </div>
  </div>


  <md-toolbar layout="row" layout-align="center center" calss="new--header">
    <div class="custom-header">
      <div class="top-header">
        <img src="img/larger-logo.png" alt="Brāv" class="logo new--brand-img">

      </div>
      <div class="top-header-right">
        <ul style="margin-top: -2px; font-size: 14px;font-weight:400;letter-spacing:0; ">
          <md-button aria-label="Chat" ng-if="showChatIcon" ng-click="toggleChatNav()" style="padding-left: 0px; margin-left: 0px; min-width: 0px;">
            <img src="img/Icons/chat.png" class="header-img" alt="chat" id="chat">
          </md-button>
          <li>
            <a href="#" id="chat">
              <img src="img/Icons/Comments.png" alt="comments" class="header-img" />
              </a/> </li>
          <li>
            <a href="#/note/create" id="notes">
              <img src="img/Icons/Note-create.png" alt="note-create" class="header-img" />
            </a>
          </li>
          <li>
            <a href="#" id="alert-msg">
              <img src="img/Icons/Notification.png" alt="notification" class="header-img" />
            </a>
          </li>
          <li>
            <md-menu>
              <md-button aria-label="Options" ng-click="$mdOpenMenu()" style="padding-left: 0px; margin-left: 0px; min-width: 0px;">
                <img src="img/user-img.jpg" class="user-img" alt="user">
              </md-button>

              <md-menu-content width="10" class="profile-edit">
                <md-menu-item md-menu-align-target>
                  <md-button ng-href="#/profile">
                    Your Account
                  </md-button>
                </md-menu-item>
                <md-menu-item>
                  <md-button ng-click="logout()" ng-href="#">
                    Logout
                  </md-button>
                </md-menu-item>
              </md-menu-content>
            </md-menu>
          </li>
        </ul>
      </div>
    </div>
  </md-toolbar>

  <md-content ng-controller="sidebarLinksCtrl" ng-init="loadSideBarLinks()" layout="row" flex>
    <md-sidenav class="md-sidenav-left white" md-component-id="left" md-whiteframe="4" md-is-locked-open="true" style="overflow:auto;z-index:0;width: 90px !important;">
      <md-list flex class="pa2">
        <md-list-item ng-click="callOpenSideNavFromParent(link)" aria-label="{{link.title}}" ng-repeat-start="link in links" ng-if="!link.disable">

          <md-button class="new--remove-btton-style" ng-href="{{link.href}}" ng-click="openSideNav(link, $index)" ng-class="{clicked: $index==clickedLinkIndex}">
            <img ng-src="{{link.src}}" class="pa1 ma0" width="40" height="40" />
            <md-tooltip md-direction="right" class="large f3 white">{{link.title}}</md-tooltip>
            <img ng-show="link.open" src="img/Icons/angle-down.png" height="10px" width="10px" />
          </md-button>

        </md-list-item>

        <md-list-item ng-show="outOfNav && link.open" ng-repeat-end ng-repeat="sublink in link.sub" aria-label="{{sublink.title}}" style="margin-left:10px;">
          <md-button style="margin: 0px; padding: 0px; min-width: 65px;" ng-class="{clicked: $index==clickedSublinkIndex}" ng-click="buttonClicked($index)">
            <a ng-href="{{sublink.href}}">
              <img ng-src="{{sublink.src}}" class="pa1 ma0" width="40" height="40" />
              <md-tooltip md-direction="right" class="large f3 white">{{sublink.title}}</md-tooltip>
            </a>
          </md-button>
        </md-list-item>
      </md-list>
    </md-sidenav>

    <md-content flex ng-init="onload()" class="ph2 brav-bg">
      <div ng-view flex>
      </div>
    </md-content>

  </md-content>

  </div>
