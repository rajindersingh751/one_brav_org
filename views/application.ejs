  <style>
    md-toolbar {
      box-shadow: 0 0 4px rgba(0, 0, 0, .14), 0 4px 8px rgba(0, 0, 0, .28) !important;
    }

    body {
      font-family: 'Raleway', sans-serif !important;
    }

    h1 {
      font-weight: 800 !important;
    }

    p, span, label{
      font-weight: 400 !important;
    }

    .logo.new--brand-img {
      width: 90px !important;
      height: 70px !important;
    }

    .new--header-icons {
      min-width: 10px;
    }

    .new--dropdown-icons {
      width: 10px;
      height: 10px;
    }

    .new--remove-btton-style {
      min-width: 45px;
    }
    .container {
      padding-right: 30px;
      padding-left: 30px;
      margin-left: 80px;
      margin-right: auto;
    }

    .md-primary, input, multi-selection, md-select {
      background-color: #ebebeb !important;
/*      height: 36px !important;*/
      /*width: 100% !important;*/
      border-radius: 3px !important; 
      border: 0 !important;
      display: block !important;
      margin-left: auto !important;
      margin-right: auto !important;
    }

    div.right-side-panel {
      margin-left: 0px;
    }

    div.col-sm-2.padding-zero.user-account {
      padding: 0px;
    }

    .create-availability {
      padding-left: 0px;
    }

    .button {
      font-weight: bold !important;
    }

    .button span {
      font-weight: bold !important;
    }

    .md-primary.button span {
      color: #ebebeb;
    }

    .md-primary.button {
      background-color: #598ebf !important;
      padding-left: 15px;
      padding-right: 15px;
    }

    div.md-errors-spacer {
       min-height: 15px;
    }

    .clicked {
      background-color: #a7a9ab;
    }

    a.md-button:not([disabled]):hover {
      background-color: transparent;
    }

    .buttons-in-row-left {
      margin-right: 10px !important;
    }

    .buttons-in-row-right {
      margin-left: 10px !important;
    }

    p.contact {
      font-weight: 700 !important;
    }

